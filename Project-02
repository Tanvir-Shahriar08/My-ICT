#include <stdio.h>
int main(){

    int A[5]={4,67,35,92,6};

    printf("Before sorting:\n");
    for(int i=0;i<5;i++){
        printf("%d\n",A[i]);
    }

//sorting
int min,temp;
for(int i=0;i<5;i++){
    min =A[i];
    for(int j=i+1;j<5;j++){
        if (min>A[j])
        {
            temp=A[i];
            A[i] = A[j];
            A[j] = temp;
        }
        
    }
}

printf("After sorting: \n");
for(int i=0;i<5;i++){
    printf("%d\n",A[i]);
}

}
